<table class="table table-striped table-inverse">
  <thead>
    <tr>
      <th>#</th>
      <th>Station Name</th>
      <th>Arrival Time</th>
      <th>Departure Time</th>
    </tr>
  </thead>
  <tbody>
  	<% @vehicle.bus_stations.each_with_index do |bus_station, index| %>
	    <tr>
	      <th scope="row"><%= index + 1%></th>
	      <td><%= bus_station.station.name%></td>
	      <td><%= bus_station.arrival_time%></td>
	      <td><%= bus_station.departure_time%></td>
	    </tr>
    <%end%>
  </tbody>
</table>
